#include <iostream>

using namespace std;

int main()
{
    int i, sum=0, n, kol=0;
    cin>>n;
    int mas [n];
    for (i=0; i<n; i++)
        cin>>mas[i];
    for (i=0; i<n; i++) {
        if (mas[i]%2 != 0) {
            sum=sum+mas[i];
            kol++;
        }
    }
    cout<<sum/kol;
    return 0;
}
